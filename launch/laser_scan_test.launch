<launch>
  <arg name="scan_topic" default="scan" />
  <node pkg="corner_detect" type="lasescan" name="laser_scan">
     <rosparam command="load" file="$(find corner_detect)/filters/shadow_filter_example.yaml" />
  </node>

  <node pkg="laser_filters" type="scan_to_scan_filter_chain"
      name="laser_filter">
    <rosparam command="load" file="$(find corner_detect)/filters/shadow_filter_example.yaml" />
    <remap from="scan" to="scan" />
  </node>

  <node name= "RosAria" pkg= "rosaria" type="RosAria">
     <remap from="RosAria/cmd_vel" to="cmd_vel"/>
     <!--<remap from="RosAria/pose" to="odom"/>-->
   </node>

   <node name= "sicklms" pkg= "sicktoolbox_wrapper" type="sicklms">
     <param name="port" type="string" value="/dev/ttyUSB1"/>
     <param name="connect_delay" value="30"/>
   </node>

  <node name="joystickcontrol" pkg="joystick_control" type="joystickcontrol"/>
  
  <node pkg="tf" type="static_transform_publisher" name="tf_static_node" args=" 0 0 0 0 0 0 base_link laser 100"/>
</launch>
